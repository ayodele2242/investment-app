            jQuery(document).ready(function($) {
              jQuery(document).on('click','.off-canvas-navigation-wrapper .navbar-toggle-hamburger',function(){
                $('.off-canvas-navigation-wrapper').addClass('toggled');
                $('body').addClass('off-canvas-active');
